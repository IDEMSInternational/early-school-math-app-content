      "type": "begin_display_group",
      "name": "dg_testing_group",
      "condition": false,
      "parameter_list": "style: column;"
    },
    {
      "name": "test_string",
      "value": "aa,bb,dd,cc,ee,de,ed"
    },
    {
      "type": "text",
      "name": "deebugg_string",
      "value": "String: @local.test_string"
    },
    {
      "name": "test_array1",
      "value": "@calc(@local.test_string.split(\",\"))"
    },
    {
      "type": "text",
      "name": "deeeebug1",
      "value": "Array1: @local.test_array1"
    },
    {
      "name": "test_set",
      "value": "@calc(new Set(@local.test_array1))"
    },
    {
      "name": "test_array2",
      "value": "@calc(Array.from(@local.test_set))"
    },
    {
      "type": "text",
      "name": "deeeebug2",
      "value": "Array2: @local.test_array2"
    },
    {
      "name": "test_array_sort",
      "value": "@calc(@local.test_array1.sort())"
    },
    {
      "type": "text",
      "name": "deeeebug_sorted",
      "value": "Array1 Sorted: @local.test_array1"
    },
    {
      "type": "text",
      "name": "deeeebug_sort",
      "value": "Array Sorted: @local.test_array_sort"
    },
    {
      "name": "test_string2",
      "value": "@calc(@local.test_array1.join(', '))"
    },
    {
      "type": "text",
      "name": "deebug_string",
      "value": "String: @local.test_string2"
    },
    {
      "name": "test_string3",
      "value": "@calc(@local.test_array2.join(', '))"
    },
    {
      "type": "text",
      "name": "deebug_string",
      "value": "String: @local.test_string3"
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "text",
      "name": "spacer"
    },
    {
      "type": "end_display_group"
    }
