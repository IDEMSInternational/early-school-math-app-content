      "rows": [
        {
          "type": "text",
          "name": "help_text",
          "value": "How do you share some mixed number amount equally among some number of people? These routines build on the ideas of wholes and fractional parts.\n\nSuppose you have 1 1/2 pies and want to share it with 6 people, how much does each person get? Use everyday things to make these routines relateable.",
          "_translations": {
            "value": {}
          },
          "parameter_list": {
            "icon_src": "help",
            "style": "navigation"
          },
          "_nested_name": "dg_info.help_text"
        }
      ],
      "_nested_name": "dg_info"
    },
    {
